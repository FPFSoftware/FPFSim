# define detector material before run initialization
/det/material LAr

/run/initialize

/gps/verbose 0
/gps/direction 0 0 1

/gps/pos/type Point
/gps/pos/centre 0 0 -2.5 m

# variables
/gps/particle gamma
/gps/ene/mono 20 GeV
/histo/saveEvd false
/histo/fileName gamma_momentum_20GeV.root
/run/beamOn 1
